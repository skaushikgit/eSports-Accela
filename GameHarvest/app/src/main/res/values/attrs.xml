<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="slideMenuStyle" format="reference" />
    <declare-styleable name="SlideMenu_Layout">
        <attr name="layout_role" format="enum">
            <enum name="content" value="0" />
            <enum name="primaryMenu" value="1" />
            <enum name="secondaryMenu" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SlideMenu">
        <attr name="primaryShadowWidth" format="dimension" />
        <attr name="secondaryShadowWidth" format="dimension" />
        <attr name="primaryShadowDrawable" format="reference" />
        <attr name="secondaryShadowDrawable" format="reference" />
        <attr name="edgeSlide" format="boolean" />
        <attr name="edgeSlideWidth" format="dimension" />
        <attr name="slideDirection">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
        </attr>
        <attr name="interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="FontTextView">
        <attr name="fontFace" format="string" />
    </declare-styleable>

    <declare-styleable name="SelectorView">
        <attr name="stringArray" format="reference" />
        <attr name="title" fromat="string"/>
    </declare-styleable>

    <declare-styleable name="OptionsView">
        <attr name="optionsArray" format="reference" />
        <attr name="title" fromat="string"/>
    </declare-styleable>

    <declare-styleable name="FormEntityView">
        <attr name="entitykey" format="string"/>
        <attr name="entityValue" format="string"/>
    </declare-styleable>

    <declare-styleable name="FormEntityCollectionView">
        <attr name="entityKeyArray" format="reference" />
        <attr name="entityValueArray" format="reference" />
    </declare-styleable>

</resources>